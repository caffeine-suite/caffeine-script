class NumberLiteralStn extends &BaseStn

  toSourceNode: (options) ->
    {value} = @props

    @createSourceNode
      if options?.dotBase
        []
          "("
          value
          ")"
      else
        value

  @getter
    propName: -> @props.value
    canBeUsedInES6Structuring: -> true

