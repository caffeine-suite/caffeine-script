import &StandardImport

class ThisStn extends &BaseStn

  needsParens: false

  @getter
    identifier: -> @children[0]?.toJs()
    propName: -> @identifier ? "this"

  toJs: ->
    if id = @identifier
      "this.#{id}"
    else
      "this"

  toSourceNode: ->
    if id = @identifier
      @createSourceNode
        :this.
        id
    else
      @createSourceNode
        :this
